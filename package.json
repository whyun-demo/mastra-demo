{
  "name": "mastra-demo",
  "version": "0.2.15",
  "description": "",
  "author": "",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.7.0"
  },
  "scripts": {
    "preinstall": "node ./scripts/preinstall.js",
    "build": "nest build",
    "build:prod": "nest build",
    "postbuild:prod": "cp package.json dist/package.json",
    "dev": "ts-node-dev --transpile-only --respawn --project tsconfig.json src/server.ts",
    "dev:env": "env-cmd -f .env ts-node-dev --transpile-only --respawn --project tsconfig.json src/server.ts",
    "dev:mastra": "mastra dev",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "eslint": "eslint . --ext .ts",
    "eslint:fix": "eslint . --ext .ts --fix",
    "start": "env-cmd -f .env nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/server.js",
    "vitest": "vitest",
    "pretest": "rm -rf coverage",
    "test": "env-cmd -f .env.test vitest",
    "pretest:ci": "rm -rf coverage",
    "test:ci": "env-cmd -f env/.env.example vitest --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.76",
    "@ai-sdk/openai-compatible": "^1.0.15",
    "@mastra/core": "1.0.0-beta.5",
    "@mastra/evals": "1.0.0-beta.1",
    "@mastra/langfuse": "1.0.0-beta.2",
    "@mastra/libsql": "1.0.0-beta.2",
    "@mastra/loggers": "1.0.0-beta.1",
    "@mastra/memory": "1.0.0-beta.2",
    "@mastra/observability": "1.0.0-beta.1",
    "@mastra/otel-exporter": "1.0.0-beta.2",
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.208.0",
    "body-parser": "^2.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "config": "^4.1.1",
    "cookie-parser": "^1.4.7",
    "jsonwebtoken": "^9.0.2",
    "node-slogger": "^3.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.3",
    "@types/body-parser": "^1.19.6",
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/utils": "^8.48.1",
    "@vitest/coverage-v8": "^4.0.15",
    "dotenv": "^17.2.3",
    "env-cmd": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.7.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-loader": "^9.5.4",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.15"
  }
}
